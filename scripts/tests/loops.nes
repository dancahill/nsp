function testloop() {
	local i=12;
	local j=0;

	print("\t");
	while (i>5) { print("[", i--, "]"); }
	print(" [", i, "]\n");
	print("\ti=");
	for (i=0;i<5;i++) { print(i); }
	print("\tj=");
	for (j=5;j>0;--j) { print(j); }
	print("\tj=");
	for (j=5;j>0;j-=1) { print(j); }
	print("\ti=");
	for (i=0;i<5;i=i+1) { if (i==3) { break; } print(i); }
	print("\n");
	print("\ti=", i, ", j=", j, "\n");
	print("\ti="); for (i=0;i<5;i++) { print(i); } print("\n");
	print("\tj="); for (j=0;j<5;j+=1) { print(j); } print("\n");
	print("\tj="); for (j=5;j>0;j--) { print(j); } print("\n");
	print("\ti="); for (i=5;i>0;i-=1) { print(i); } print("\n");
	return;
}
testloop();
