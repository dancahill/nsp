#!/usr/bin/nesla
if (type(expect)!='function') include("_expect.nes");
function testif() {
	local x=2;
	local str="";

	if (x==1) {
		str=str+"test 1 (bad)";
	} else if (x==2) {
		str=str+"test 2 (good)";
	} else {
		str=str+"test 3 (bad)";
	}
	expect("simple 'if' test", "test 2 (good)", str);
	return;
}
testif();
