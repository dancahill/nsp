function teststring() {
	local x=42/5;

	print(
		"\ttesting ",
		"multi
		line output (", x, ") (", pi, ")\n"
	);
	local x=string.cat(string.cat("Aa", "Bb"), "CcDd");
	print("\tx=", x, " - ", string.sub(x, 1, 2), string.sub(x, 4, 1), "\n");
	print("\tstring.len(x)      expect               [ 8 ] : [ ", string.len(x), " ]\n");
	print("\tstring.cmp(x)      expect             [ -32 ] : [ ", string.cmp(x, "aabbccdd"), " ]\n");
	print("\tstring.icmp(x)     expect               [ 0 ] : [ ", string.icmp(x, "aabbccdd"), " ]\n");
	print("\tstring.ncmp(x)     expect             [ -32 ] : [ ", string.ncmp(x, "aabb", 4), " ]\n");
	print("\tstring.nicmp(x)    expect               [ 0 ] : [ ", string.nicmp(x, "aabb", 4), " ]\n");
	print("\tstring.sub(x)      expect            [ is a ] : [ ", string.sub("this is a test.", 5, 4), " ]\n");
	print("\tstring.sub(x)      expect            [ test ] : [ ", string.sub("this is a test.", -5, 4), " ]\n");

	print("1 ", string.str("this is a Test of something useful", "test"), "\n");
	print("2 ", string.istr("this is a Test of something useful", "test"), "\n");
	print("3 ", string.str("this is a test of something useful", " is"), "\n");
	x="1|2|3|4|5|6|7";
	print(x, "\n");
	x=string.sub(string.str(x, "|"), 1, 42);
	print(x, "\n");
	x=string.sub(string.str(x, "|"), 1, 42);
	print(x, "\n");
	x=string.sub(string.str(x, "|"), 1, 42);
	print(x, "\n");
	x=string.sub(string.str(x, "|"), 1, 42);
	print(x, "\n");
	x=string.sub(string.str(x, "|"), 1, 42);
	print(x, "\n");
	x=string.sub(string.str(x, "|"), 1, 42);
	print(x, "\n");
	x=string.sub(string.str(x, "|"), 1, 42);
	print(x, "\n");

	return;
}
teststring();
